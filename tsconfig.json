{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "watch": false,
    "outDir": "wwwroot/app"
  },
  "exclude": [
    "node_modules",
    "typings/index",
    "typings/index.d.ts"
  ],
  "compileOnSave": true
}